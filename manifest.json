{
  "manifest_version": 2,
  "author": "CloudKeeper",
  "homepage_url": "https://pro.cloudkeeper.ai",
  "name": "CloudKeeper - Credential Helper",
  "description": "AWS SSO External AWS Account - STS Keys Generator",
  "version": "1.2.0",
  "icons": {
    "32": "icons/icon_crop_128.png",
    "128": "icons/icon_crop_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_crop_128.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "page": "background/background.html"
  },
  "permissions": [
    "webRequest",
    "storage",
    "webNavigation",
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "ckfirefoxextension@tothenew.com",
      "strict_min_version": "52.0"
    }
  }
}